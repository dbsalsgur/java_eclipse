package ccsp.models;
//package ccsp.model;

import java.sql.Date;

public class CrimeRecordVO {
	private int crimNo;
	private int region;
	private int sex;
	private String cRecord;
	private String name;
	private int regitNumber;
	private Date date;
	private int crimDivNo;
	private String regionName;
	private String categoryName; // 카테고리 테이블의 category 컬럼
	private String categoryDes; // 카테고리 테이블의 디스크립션 컬럼
	private String sexName; // MALE or FEMALE

	public CrimeRecordVO() {
	}

	public CrimeRecordVO(int crimNo, int region, int sex, String cRecord, String name, int regitNumber, Date date,
			int crimDivNo) {
		super();
		this.crimNo = crimNo;
		this.region = region;
		this.sex = sex;
		this.cRecord = cRecord;
		this.name = name;
		this.regitNumber = regitNumber;
		this.date = date;
		this.crimDivNo = crimDivNo;
	}

	public int getCrimNo() {
		return crimNo;
	}

	public void setCrimNo(int crimNo) {
		this.crimNo = crimNo;
	}

	public int getRegion() {
		return region;
	}

	public void setRegion(int region) {
		this.region = region;
	}

	public int getSex() {
		return sex;
	}

	public void setSex(int sex) {
		this.sex = sex;
	}

	public String getcRecord() {
		return cRecord;
	}

	public void setcRecord(String cRecord) {
		this.cRecord = cRecord;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getRegitNumber() {
		return regitNumber;
	}

	public void setRegitNumber(int regitNumber) {
		this.regitNumber = regitNumber;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}

	public int getCrimDivNo() {
		return crimDivNo;
	}

	public void setCrimDivNo(int crimDivNo) {
		this.crimDivNo = crimDivNo;
	}

	public String getRegionName() {
		return regionName;
	}

	public void setRegionName(String regionName) {
		this.regionName = regionName;
	}

	public String getCategoryName() {
		return categoryName;
	}

	public void setCategoryName(String categoryName) {
		this.categoryName = categoryName;
	}

	public String getCategoryDes() {
		return categoryDes;
	}

	public void setCategoryDes(String categoryDes) {
		this.categoryDes = categoryDes;
	}

	public String getSexName() {
		return sexName;
	}

	public void setSexName(String sexName) {
		this.sexName = sexName;
	}

}
